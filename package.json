{
  "name": "yarsk",
  "version": "1.0.0",
  "description": "My React starter kit. Webpack, Babel, Sass, React 0.13 and more.",
  "main": "app/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start conf/karma.conf.js --single-run",
    "test:watch": "./node_modules/karma/bin/karma start conf/karma.conf.js",
    "test:coverage": "./node_modules/karma/bin/karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "./node_modules/karma/bin/karma start conf/karma.ci.conf.js --single-run",
    "start": "parallelshell 'gulp' 'npm run test:watch' './node_modules/.bin/webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --inline --content-base ./build'",
    "build": "./node_modules/.bin/webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist"
  },
  "author": "Brad Daily <brad@bradleyboy.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.8",
    "react": "^0.13",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.1.10",
    "babel-core": "^5.4",
    "babel-eslint": "^3.1.7",
    "babel-loader": "^5.0.0",
    "beepbeep": "^1.2.0",
    "bourbon": "^4.2.3",
    "chai": "^2.3.0",
    "chalk": "^1.0.0",
    "css-loader": "^0.12",
    "eslint": "0.21.0",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-bdd": "^1.1.0",
    "eslint-plugin-react": "^2.3.0",
    "extract-text-webpack-plugin": "^0.8",
    "file-loader": "^0.8.1",
    "gh-pages": "^0.2.0",
    "gulp": "^3.8.11",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.2.1",
    "gulp-eslint": "^0.12.0",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-watch": "^4.2.4",
    "html-webpack-plugin": "^1.1.0",
    "isparta": "^2.2.0",
    "isparta-instrumenter-loader": "^0.1.1",
    "karma": "^0.12.32",
    "karma-babel-preprocessor": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-notify-reporter": "^0.1.1",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5",
    "node-notifier": "^4.2.1",
    "notifier": "^0.1.7",
    "parallelshell": "^1.1.1",
    "react-hot-loader": "^1.2.3",
    "sass-loader": "^0.4.2",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.9",
    "webpack-dev-server": "^1.7.0"
  }
}
